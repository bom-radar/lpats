set(PACKAGE_VERSION "@LPATS_VERSION@")

# enforce major version match
if ("${PACKAGE_FIND_VERSION}" VERSION_GREATER @LPATS_VERSION@ OR NOT "${PACKAGE_FIND_VERSION_MAJOR}" EQUAL @LPATS_VERSION_MAJOR@)
  set(PACKAGE_VERSION_COMPATIBLE FALSE)
  set(PACKAGE_VERSION_EXACT FALSE)
else()
  set(PACKAGE_VERSION_COMPATIBLE TRUE)
  if ("${PACKAGE_FIND_VERSION}" VERSION_EQUAL @LPATS_VERSION@)
    set(PACKAGE_VERSION_EXACT TRUE)
  endif()
endif()
