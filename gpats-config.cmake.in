# Config file for the gpats package
# Defines the following variables:
#  GPATS_INCLUDE_DIRS - include directories for gpats
#  GPATS_LIBRARIES    - libraries to link against

# compute paths
get_filename_component(GPATS_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(GPATS_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# out library dependencies
include("${GPATS_CMAKE_DIR}/gpats-targets.cmake")

# imported targets created by gpats-targets.cmake
set(GPATS_LIBRARIES gpats)
